<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zt.ssm.mapper.EmployeeMapper">
    <!--  接口的全类名和namespace的值相同  -->
    <select id="getEmployee" resultType="com.zt.ssm.bean.Employee">
        select *
        from employee
        where id = #{id}
    </select>
</mapper>